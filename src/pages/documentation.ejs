<h1 class="dark-blue mt3">Documentation</h1>
<h2 class="h4 mb1 light-gray-text">Table of Contents (under development)!</h2>
<ol class="no-bullets m0 pl0 lh">
  <% for (var i = postsDataForIndexPage.length - 1, j = 0; i >= 0; i--) { %>
  <li class="mbhalf"><a href="<%= postsDataForIndexPage[i].pathToPost %>">
      <%- j + 1 %>.
      <%= postsDataForIndexPage[i].title %></a></li>
  <% j = j + 1 %>
  <% } %>
</ol>

